@{
    ViewData["Title"] = "CSharp29 Yearbook📎";
}
<head>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Abril+Fatface&family=Inter+Tight:wght@100&display=swap" rel="stylesheet">
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

</head>

<body>
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel" data-pause="hover" data-interval=8000 data-keyboard=true data-touch = true>
        <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-100" src="/images/Graduation1.jpg" alt="First slide" >
                <div class="carousel-caption">
                    <h1 class="animate__animated animate__bounceInLeft animate__slow">HEY CSHARP29 FAM!</h1>
                    <h2 class="animate__animated animate__bounceInRight animate__slow animate__delay-2s">Can you believe that we are graduating already?!</h2>
                    <h2 class="animate__animated animate__rotateIn animate__delay-4s">Congratulations to us!!!🥳🎉</h2>
                </div>
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="/images/Graduation2.jpg" alt="Second slide">
                <div class="carousel-caption">
                    <h2 class="animate__animated animate__fadeInTopLeft animate__slow">To commemorate this special event, I've created a yearbook for our class. </h2>
                    <h2 class="animate__animated animate__fadeInBottomRight animate__slower animate__delay-2s">So make sure to sign your name and check out your classmates' names! </h2>
                </div>
            </div>
            <div class="carousel-item">
                 <img class="d-block w-100" src="/images/Graduation3.jpeg" alt="Third slide" >
                <div class="carousel-caption">
                    <h1 class="animate__animated animate__zoomIn animate__slow">Cheers to hard work, dedication, and constantly improving yourself🙃 </h1>
                    <h1 class="animate__animated animate__bounceIn animate__slow animate__delay-3s">     Let's celebrate!!!🎊🎈 </h1>
                    <div>
                        <button class="sign" type="button"><a href="/Student/Index">Leave your legacy today🎓</a></button>

                    </div>
                       
                    
                </div>
                
            </div>

        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
    </div>
   


</body>
<foot>
    <div><audio controls autoplay> <source src="/lib/GraduationSong.mp3" type="audio/mp3"></audio></div>
</foot>







