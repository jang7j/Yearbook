@model IEnumerable<Student>

  

<h2>CSharp29 Fam</h2>
<div style="display: block">
    <a href="/Student/CreateStudent/">Sign your name</a>
</div>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Student ID</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Picture</th>
      <th scope="col">Gender</th>
      <th scope="col">State</th>
      <th scope="col">Email</th>
      <th scope="col">Hobbies</th>
      <th scope="col">Favorite Food</th>
      <th scope="col">Favorite Song</th>
      <th scope="col">Favorite Movie</th>
      <th scope="col">Future Career</th>
      <th scope="col">Quote</th>
    </tr>
  </thead>
    
    
  
    @foreach (var student in Model)
    {
    <tr>
        <td><a href=/Student/ViewStudent/@student.StudentID>@student.StudentID</a></td>
        <td>@student.FirstName</td>
        <td>@student.LastName</td>
        <td>@student.Picture</td>
        <td>@student.Gender</td>
        <td>@student.States</td>
        <td>@student.Email</td>
        <td>@student.Hobbies</td>
        <td>@student.FavoriteFood</td>
        <td>@student.FavoriteSong</td>
        <td>@student.FavoriteMovie</td>
        <td>@student.FutureCareer</td>
        <td>@student.Quote</td>
    </tr>
    
    }
</table>

 