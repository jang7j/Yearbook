@model IEnumerable<Student>

  

<h2>CSharp29 Fam</h2>
<div >
    <button class="glow-on-hover" type="button"><a href="/Student/CreateStudent/">Sign your name</a> </button>
    </div>


<table class="table table-hover">
    
        <tr>
            @*<th scope="col">#</th>*@
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Picture </th>
            <th scope="col">Gender</th>
            <th scope="col">State</th>
            <th scope="col">Email</th>
            <th scope="col">Hobbies</th>
            <th scope="col">Favorite Food</th>
            <th scope="col">Favorite Song</th>
            <th scope="col">Favorite Movie</th>
            <th scope="col">Future Career</th>
            <th scope="col" colspan="3">Quote</th>
        </tr>
   

    @foreach (var student in Model)
    {
    
        <tr>
            @*<td scope="row"><a href=/Student/ViewStudent/@student.StudentID>@student.StudentID</a></td>*@
            <td scope="row"><a href=/Student/ViewStudent/@student.StudentID>@student.FirstName</td>
            <td scope="row">@student.LastName</td>
            <td scope="row">@student.Image</td>
            <td scope="row">@student.Gender</td>
            <td scope="row">@student.State</td>
            <td scope="row">@student.Email</td>
            <td scope="row">@student.Hobbies</td>
            <td scope="row">@student.FavoriteFood</td>
            <td scope="row">@student.FavoriteSong</td>
            <td scope="row">@student.FavoriteMovie</td>
            <td scope="row">@student.FutureCareer</td>
            <td scope="row" colspan="3">@student.Quote</td>
        </tr>
   
        
    }
</table>
 